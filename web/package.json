{
  "name": "wasteland-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "check": "tsc --noEmit && biome check src/ && vitest run --passWithNoTests"
  },
  "dependencies": {
    "cmdk": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.0",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "jsdom": "^28.1.0",
    "typescript": "~5.7.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
